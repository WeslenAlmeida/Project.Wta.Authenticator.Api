{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "AllowedHosts": "*",
    "TokenConfiguration":{
      "Audience":"Audience",
      "Issuer":"Issuer",
      "Seconds": 3600
    },
    "MongoSettings":{
      "DatabaseName":"${MongoSettings.DatabaseName}",
      "ConnectionString":"${MongoSettings.ConnectionString}"
    },
    "RedisSettings":{
      "TimeExpiration":3600,
      "ConnectionString":"${RedisSettings.ConnectionString}"
    },
    "Authorization":{
      "ApiKeyName" : "key",
      "ApiKey" : "${Authorization.ApiKey}"
    }
  }
  