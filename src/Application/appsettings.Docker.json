{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "AllowedHosts": "*",
    "TokenConfiguration":{
      "Audience":"Audience",
      "Issuer":"Issuer",
      "Seconds": "${RedisSettings.TimeExpiration}"
    },
    "MongoSettings":{
      "DatabaseName":"${MongoSettings.DatabaseName}",
      "ConnectionString":"${MongoSettings.ConnectionString}"
    },
    "RedisSettings":{
      "TimeExpiration":"${RedisSettings.TimeExpiration}",
      "ConnectionString":"${RedisSettings.ConnectionString}"
    },
    "Authorization":{
      "ApiKeyName" : "key",
      "ApiKey" : "${Authorization.ApiKey}"
    }
  }
  